{
  "name": "path-equal",
  "version": "0.0.0-development",
  "description": "Check if the file path are equal",
  "homepage": "https://github.com/unional/path-equal",
  "bugs": {
    "url": "https://github.com/unional/path-equal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/path-equal.git"
  },
  "license": "MIT",
  "files": [
    "dist-es5"
  ],
  "main": "dist-es5/index.js",
  "typings": "dist-es5/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build-es5",
    "build-es5": "tsc -p tsconfig.base.json",
    "clean": "rimraf dist-es5",
    "coverage": "jest --coverage",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "tsc && jest",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": ".*.spec.ts$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "dependency-check": "^3.2.1",
    "eslint": "^5.7.0",
    "eslint-plugin-unional": "^1.0.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.10.0",
    "typescript": "^3.1.3"
  }
}
